---
layout: null
robots: YES
---
[
  {% for post in site.travel %}
    {
      "title"    : "{{ post.title | escape }}",
      "category" : "{{ post.tara }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}",
	    "image"     : "{{ post.image }}",
			"author"    : "{{ post.author }}",
	    "description"     : "{{ post.description }}",
			"cat"    : "{{ post.cat }}",
      "subcat" : "{{ post.subcat }}",
      "subcat1"     : "{{ post.subcat1 }}",
			"type"     : "travel"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
	,
  {% for post in site.new-travel %}
    {
      "title"    : "{{ post.title | escape }}",
      "category" : "{{ post.tara }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}",
	    "image"     : "{{ post.image }}",
			"author"    : "{{ post.author }}",
	    "description"     : "{{ post.description | escape }}",
			"cat"    : "{{ post.cat }}",
			"type"     : "travel"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
	,
	{% for post in site.stiri %}
    {
      "title"    : "{{ post.title | escape }}",
      "category" : "{{ post.categorie }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}",
	    "image"     : "/assets/images/{{ post.image_path }}",
			"author"    : "{{ post.author }}",
	    "description"     : "{{ post.description }}",
			"cat"    : "{{ post.cat }}",
      "subcat" : "{{ post.subcat }}",
      "subcat1"     : "{{ post.subcat1 }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
	,
	{% for post in site.smarthome %}
    {
      "title"    : "{{ post.title | escape }}",
      "category" : "{{ post.categorie }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}",
	    "image"     : "/assets/images/{{ post.image_path }}",
			"author"    : "{{ post.author }}",
	    "description"     : "{{ post.description }}",
			"cat"    : "{{ post.cat }}",
      "subcat" : "{{ post.subcat }}",
      "subcat1"     : "{{ post.subcat1 }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
	,
	{% for post in site.manuka %}
    {
      "title"    : "{{ post.title | escape }}",
      "category" : "{{ post.categorie }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}",
	    "image"     : "/assets/images/{{ post.image_path }}",
			"author"    : "{{ post.author }}",
	    "description"     : "{{ post.description }}",
			"cat"    : "{{ post.cat }}",
      "subcat" : "{{ post.subcat }}",
      "subcat1"     : "{{ post.subcat1 }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
	,
  {% for post in site.general %}
    {
      "title"    : "{{ post.title | escape }}",
      "category" : "{{ post.categorie }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}",
	    "image"     : "/assets/images/{{ post.image_path }}",
			"author"    : "{{ post.author }}",
	    "description"     : "{{ post.description }}",
			"cat"    : "{{ post.cat }}",
      "subcat" : "{{ post.subcat }}",
      "subcat1"     : "{{ post.subcat1 }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
	,
	{% for post in site.reviews %}
    {
      "title"    : "{{ post.title | escape }}",
      "category" : "{{ post.topic | upcase}}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}",
	    "image"     : "/{{ post.meta_image }}",
			"author"    : "{{ post.author }}",
	    "description"     : "{{ post.description }}",
      "score"     : "{{ post.rating-value }}",
			"cat"    : "{{ post.cat }}",
      "subcat" : "{{ post.subcat }}",
      "subcat1"     : "{{ post.subcat1 }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
  ,
  {% for post in site.new-reviews %}
    {
      "title"    : "{{ post.title | escape }}",
      "category" : "{{ post.topic | upcase}}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}",
      {% if post.meta_image != "" %}"image"     : "/{{ post.meta_image }}",{% else %}"image"     : "{{ post.imagine-transparent-mobile }}",
      {% endif %}
			"author"    : "{{ post.author }}",
	    "description"     : "{{ post.description }}",
      "score"     : "{{ post.rating-value }}",
			"cat"    : "{{ post.cat }}",
      "subcat" : "{{ post.subcat }}",
      "subcat1"     : "{{ post.subcat1 }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]

