---
layout: null
---
[
  {% for trip in site.new-travel reversed %}
	{
		"titlu": "{{ trip.title }}",
		"data": "{{ trip.date | date_to_string }}",
		"imagine": "/assets/images/travel/{{trip.folder}}/{% if trip.mobile_main_image %}{{ trip.mobile_main_image }}{% else %}{{ trip.imagine }}{% endif %}",
		"link": "{{ trip.url }}",
		"oras": "{{ trip.oras }}",
		"categorie": "{{ trip.tara }}",
		"descriere": "{{ trip.descriere | escape }}",
		"tags": [{% for tag in trip.tags %}"{{ tag }}"{% unless forloop.last %},{% endunless %}{% endfor %}]
	},{% endfor %}
	{% for trip in site.travel reversed %}
	{
		"titlu": "{{ trip.title }}",
		"data": "{{ trip.date | date_to_string }}",
		"imagine": "{{ trip.image | replace: ".webp", "_mobile.webp" }}",
		"link": "{{ trip.url }}",
		"oras": "{{ trip.oras }}",
		"categorie": "{{ trip.tara }}",
		"descriere": "{{ trip.descriere }}",
		"tags": [{% for tag in trip.tags %}"{{ tag }}"{% unless forloop.last %},{% endunless %}{% endfor %}]
	}{% unless forloop.last %},{% endunless %}{% endfor %}
]
